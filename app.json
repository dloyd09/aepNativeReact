{
  "expo": {
    "name": "WeRetail",
    "slug": "WeRetail",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/productImages/weretail-logo.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/productImages/weretail-logo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.WeRetail"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/productImages/weretail-logo.svg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.WeRetail"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
     ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
