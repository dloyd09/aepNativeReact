{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "RCT_NEW_ARCH_ENABLED": "0",
        "EXPO_NEW_ARCH_ENABLED": "0"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "env": {
        "RCT_NEW_ARCH_ENABLED": "0",
        "EXPO_NEW_ARCH_ENABLED": "0",
        "RCT_FABRIC_ENABLED": "0"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store",
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "env": {
        "RCT_NEW_ARCH_ENABLED": "0",
        "EXPO_NEW_ARCH_ENABLED": "0",
        "RCT_FABRIC_ENABLED": "0"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
